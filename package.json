{
  "name": "@ls-stack/cli",
  "description": "CLI input utilities for interactive prompts",
  "version": "0.5.0",
  "license": "MIT",
  "scripts": {
    "test": "tsx scripts/test-cli.ts",
    "lint": "pnpm tsc && pnpm eslint",
    "tsc": "tsc -p tsconfig.prod.json",
    "eslint": "CI=true eslint src/ scripts/ --color --max-warnings=0",
    "build": "pnpm lint && tsdown",
    "build:no-test": "tsdown",
    "pre-publish": "pnpm build"
  },
  "files": [
    "dist"
  ],
  "repository": "github:lucasols/cli",
  "author": "Lucas Santos",
  "type": "module",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.mts",
        "default": "./dist/main.mjs"
      }
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=21.5.0"
  },
  "devDependencies": {
    "@ls-stack/eslint-cfg": "^0.5.1",
    "@ls-stack/pkg-manager": "^0.2.1",
    "@ls-stack/utils": "^3.68.0",
    "@types/node": "^24.7.1",
    "prettier": "3.6.2",
    "tsdown": "^0.20.2",
    "tsx": "^4.19.4",
    "typescript": "5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.5.1",
    "nanospinner": "^1.2.2"
  }
}
